{
  "name": "captain-app",
  "version": "0.0.1",
  "description": "Desktop GUI for Captain - Conservation Area Prioritization Through Artificial INtelligence",
  "private": true,
  "author": "Daniel Edler, Anton Eriksson",
  "homepage": "https://www.captain-project.net/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/captain-project/captain-app.git"
  },
  "bugs": {
    "url": "https://github.com/captain-project/captain-app/issues"
  },
  "keywords": [
    "Electron",
    "GUI",
    "Biodiversity",
    "Conservation"
  ],
  "engines": {
    "node": ">=v16.13",
    "npm": ">=8.1"
  },
  "main": "packages/main/dist/index.cjs",
  "types": "lib/",
  "scripts": {
    "build": "node scripts/build.js",
    "precompile": "cross-env MODE=production npm run build",
    "compile": "electron-builder build --config .electron-builder.config.js --dir --config.asar=false",
    "jest": "jest --forceExit --no-cache",
    "pretest": "npm run build",
    "testOrig": "node tests/app.spec.js",
    "test": "echo playwright required",
    "watch": "node scripts/watch.js",
    "lint": "eslint . --ext js,ts,tsx",
    "typecheck-main": "tsc --noEmit -p packages/main/tsconfig.json",
    "typecheck-preload": "tsc --noEmit -p packages/preload/tsconfig.json",
    "typecheck-renderer": "tsc --noEmit -p packages/renderer/tsconfig.json",
    "typecheck": "npm run typecheck-main && npm run typecheck-preload && npm run typecheck-renderer",
    "pretypecheck-renderer": "dts-cb -i packages/preload/src/**/*.ts -o packages/preload/exposedInMainWorld.d.ts"
  },
  "standard": {
    "env": [
      "jest"
    ],
    "ignore": []
  },
  "devDependencies": {
    "@types/electron-devtools-installer": "2.2.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/serve-favicon": "^2.5.3",
    "@types/sharp": "^0.29.5",
    "@types/svgo": "^2.6.1",
    "@typescript-eslint/eslint-plugin": "5.8.0",
    "@vitejs/plugin-react": "^1.1.3",
    "axios": "^0.25.0",
    "cross-env": "7.0.3",
    "dts-for-context-bridge": "0.7.1",
    "electron": "16.0.5",
    "electron-builder": "22.14.5",
    "electron-devtools-installer": "3.2.0",
    "eslint": "8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.28.0",
    "jest": "^27.4.7",
    "nano-staged": "0.5.0",
    "prettier": "^2.5.1",
    "simple-git-hooks": "2.7.0",
    "ts-jest": "^27.1.3",
    "typescript": "4.5.4",
    "vite": "2.7.7"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.7.3",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@feathersjs/client": "^5.0.0-pre.16",
    "@feathersjs/express": "^5.0.0-pre.16",
    "@feathersjs/feathers": "^5.0.0-pre.16",
    "@feathersjs/socketio": "^5.0.0-pre.16",
    "@feathersjs/socketio-client": "^5.0.0-pre.16",
    "@typescript-eslint/parser": "^5.8.0",
    "cors": "^2.8.5",
    "electron-updater": "4.6.5",
    "framer-motion": "^5.5.5",
    "mobx": "^6.3.10",
    "mobx-react": "^7.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-router-dom": "^6.2.1",
    "serve-favicon": "^2.5.0",
    "sharp": "^0.29.3",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.4.1",
    "svgo": "^2.8.0",
    "winston": "^3.4.0"
  }
}
